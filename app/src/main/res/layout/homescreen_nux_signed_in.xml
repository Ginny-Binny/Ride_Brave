<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white"
    tools:context=".MainActivity">

    <TextView
        android:layout_width="104dp"
        android:layout_height="29dp"
        android:text="Hi Komal!"
        android:textStyle="bold"
        android:textColor="#373737"
        android:textSize="24sp"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteY="39.43dp"
        tools:layout_editor_absoluteX="24.44dp"/>

    <ImageView
        android:id="@+id/picture"
        android:layout_width="49.37dp"
        android:layout_height="49.37dp"
        android:background="@drawable/picture"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteY="30.74dp"
        tools:layout_editor_absoluteX="320.18dp"/>
    <ImageView
        android:id="@+id/location"
        android:layout_width="12.6dp"
        android:layout_height="18dp"
        android:background="@drawable/location"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="24dp"
        tools:layout_editor_absoluteY="80dp" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="149dp"
        android:layout_height="29dp"
        android:text="Gharuan"
        android:textColor="#37373780"
        android:textSize="24sp"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="43.29dp"
        tools:layout_editor_absoluteY="81.11dp" />

    <ImageView
        android:id="@+id/imageView8"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@drawable/nav_bar"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteY="623dp" />

    <ImageView
        android:layout_width="45dp"
        android:layout_height="51.77dp"
        android:background="@drawable/homefilled"
        tools:layout_editor_absoluteX="37dp"
        tools:layout_editor_absoluteY="663dp"
        tools:ignore="MissingConstraints" />

    <ImageView
        android:layout_width="45dp"
        android:layout_height="51.77dp"
        android:background="@drawable/requestempty"
        tools:layout_editor_absoluteX="128dp"
        tools:layout_editor_absoluteY="663dp"
        tools:ignore="MissingConstraints" />

    <ImageView
        android:layout_width="65dp"
        android:layout_height="51.77dp"
        android:background="@drawable/pastjourneysempty"
        tools:layout_editor_absoluteX="204dp"
        tools:layout_editor_absoluteY="663dp"
        tools:ignore="MissingConstraints" />

    <ImageView
        android:layout_width="45dp"
        android:layout_height="51.77dp"
        android:background="@drawable/profileempty"
        tools:layout_editor_absoluteX="308dp"
        tools:layout_editor_absoluteY="663dp"
        tools:ignore="MissingConstraints" />

    <ImageView
        android:id="@+id/sos"
        android:layout_width="59.37dp"
        android:layout_height="59.37dp"
        android:background="@drawable/sos"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="320dp"
        tools:layout_editor_absoluteY="573dp" />

    <TextView
        android:layout_width="169dp"
        android:layout_height="19dp"
        android:textSize="15.94sp"
        android:text="Welcome to RideBrave!"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/imageView3"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView5"
        app:layout_constraintVertical_bias="0.811" />

    <ImageView
        android:id="@+id/imageView9"
        android:layout_width="232.82dp"
        android:layout_height="64.97dp"
        android:background="@drawable/rectangle_empty"
        app:layout_constraintBottom_toTopOf="@+id/imageView8"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.505"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView7"
        app:layout_constraintVertical_bias="0.13" />

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="232.82dp"
        android:layout_height="64.97dp"
        android:background="@drawable/rectangle_empty"
        app:layout_constraintBottom_toTopOf="@+id/imageView7"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView5"
        app:layout_constraintVertical_bias="0.894" />

    <ImageView
        android:id="@+id/imageView7"
        android:layout_width="232.82dp"
        android:layout_height="64.97dp"
        android:background="@drawable/rectangle_empty"
        app:layout_constraintBottom_toTopOf="@+id/imageView8"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView5" />

    <TextView
        android:layout_width="216.74dp"
        android:layout_height="35dp"
        android:textSize="13.48sp"
        android:text="Click on “Travel Today” to find a compamion for your train trip!"
        app:layout_constraintBottom_toBottomOf="@+id/imageView3"
        app:layout_constraintEnd_toEndOf="@+id/imageView3"
        app:layout_constraintStart_toStartOf="@+id/imageView3"
        app:layout_constraintTop_toTopOf="@+id/imageView3" />

    <TextView
        android:layout_width="222.74dp"
        android:layout_height="39dp"
        android:textSize="13.48sp"
        android:text="View your requests that are accepted or pending from your end."
        app:layout_constraintBottom_toBottomOf="@+id/imageView7"
        app:layout_constraintEnd_toEndOf="@+id/imageView7"
        app:layout_constraintStart_toStartOf="@+id/imageView7"
        app:layout_constraintTop_toTopOf="@+id/imageView7" />

    <TextView
        android:layout_width="216.74dp"
        android:layout_height="50dp"
        android:textSize="13.48sp"
        android:text="You also have the option of SOS and side amenities like Hospital , etc."
        app:layout_constraintBottom_toBottomOf="@+id/imageView9"

        app:layout_constraintEnd_toEndOf="@+id/imageView9"
        app:layout_constraintStart_toStartOf="@+id/imageView9"
        app:layout_constraintTop_toTopOf="@+id/imageView9" />


</androidx.constraintlayout.widget.ConstraintLayout>
